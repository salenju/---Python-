   ],
   "source": [
    "# 绘制sin函数曲线\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 生成数据\n",
    "x = np.arange(0, 6, 0.1)    # 以0.1为单位，生成0-6的数据\n",
    "y = np.sin(x)\n",
    "\n",
    "# 绘制图形\n",
    "plt.plot(x, y)\n",
    "plt.show()"
   ]
  },
